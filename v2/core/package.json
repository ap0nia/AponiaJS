{
  "name": "aponia-v2",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "build": "rimraf dist && tsc",
    "prepublish": "npm run build"
  },
  "files": [
    "src",
    "dist"
  ],
  "main": "dist/src/index.js",
  "module": "dist/src/index.js",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "require": "./dist/src/index.cjs",
      "default": "./dist/src/index.js"
    },
    "./providers/*": {
      "types": "./dist/providers/*.d.ts",
      "require": "./dist/providers/*.cjs",
      "default": "./dist/providers/*.js"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./dist/src/index.d.ts",
        "./src/aponia-auth.d.ts"
      ],
      "./providers/*": [
        "./dist/providers/*.d.ts"
      ]
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@panva/hkdf": "^1.0.4",
    "cookie": "^0.5.0",
    "defu": "^6.1.2",
    "jose": "^4.13.1",
    "oauth4webapi": "^2.2.0",
    "rimraf": "^5.0.0"
  },
  "devDependencies": {
    "@types/cookie": "^0.5.1",
    "@types/node": "^18.15.11",
    "typescript": "^5.0.4"
  }
}
